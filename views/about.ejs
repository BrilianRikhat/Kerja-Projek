<script>
  let increment = 0;
  const img = document.querySelectorAll(".container-img img");
  let interval =   setInterval(() => {
           
             img[increment].style.animationName = "translate";
            img[increment].style.animationDuration = "2s";
            console.log(increment = (increment + 1) % img.length);

           } , 1000);
 
           setTimeout(() => {
    clearInterval(interval);
    img.forEach(e => {
        e.style.animationName = "translate";
        e.style.animationDuration = "2s";
        e.style.animationIterationCount = "infinite";  // Fixed typo here
        e.style.animationDirection = "alternate";
    });
}, 4000);
 
</script>


